{
  "name": "dataengineerhub-blog",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview --host",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "deploy": "npm run build && npm run deploy:netlify",
    "deploy:netlify": "netlify deploy --prod --dir dist",
    "optimize:images": "node scripts/optimize-images.js",
    "perf:audit": "lighthouse http://localhost:4173 --output html --output-path lighthouse-report.html",
    "perf:build": "npm run build && npm run preview & sleep 5 && npm run perf:audit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-helmet-async": "^1.3.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.268.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "vite": "^4.4.5",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "tailwindcss": "^3.3.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.28",
    "cssnano": "^6.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.16.4",
    "workbox-window": "^7.0.0",
    "sharp": "^0.32.5",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "lighthouse": "^10.4.0",
    "vitest": "^0.34.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "npm@8.19.2"
}